{
  "name": "slack-channel-active-reminder",
  "description": "remind non acctive channel at slack",
  "version": "0.1.0",
  "author": "github.com/9renpoto",
  "bugs": {
    "url": "https://github.com/9renpoto/slack-channel-active-reminder/issues"
  },
  "dependencies": {
    "@slack/client": "^3.14.2",
    "config": "^1.28.1",
    "moment": "^2.19.4"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "^0.6.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.5",
    "documentation": "^5.3.5",
    "eslint": "^4.13.1",
    "flow-bin": "^0.61.0",
    "ghpages": "^0.0.10",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.4.2",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=7.10"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/9renpoto/slack-channel-active-reminder#readme",
  "keywords": [
    "slack"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/slack-channel-active-reminder.git"
  },
  "scripts": {
    "build": "mkdir -p docs lib && run-p build:*",
    "build:doc": "documentation build src/** -f html -o docs",
    "build:js": "cross-env NODE_ENV=production babel src -d lib",
    "clean": "rimraf docs lib",
    "lint": "run-p lint:*",
    "lint:documentation": "documentation lint src",
    "lint:es": "eslint . --cache",
    "lint:flow": "flow",
    "postpublish": "ghpages --path docs",
    "prebuild": "npm run clean",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "start": "node lib/index.js",
    "test": "run-p test:*",
    "test:lint": "npm run lint"
  }
}
