{
  "name": "slack-channel-active-reminder",
  "description": "remind non acctive channel at slack",
  "version": "0.0.2",
  "author": "github.com/9renpoto",
  "bugs": {
    "url": "https://github.com/9renpoto/slack-channel-active-reminder/issues"
  },
  "dependencies": {
    "@slack/client": "^3.9.0",
    "config": "^1.26.1",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "^0.3.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "documentation": "^5.0.0",
    "eslint": "^4.0.0",
    "flow-bin": "^0.51.0",
    "ghpages": "^0.0.10",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1"
  },
  "engines": {
    "node": ">=7.10"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/9renpoto/slack-channel-active-reminder#readme",
  "keywords": [
    "slack"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/slack-channel-active-reminder.git"
  },
  "scripts": {
    "build": "mkdir -p docs lib && npm-run-all -p build:*",
    "build:doc": "documentation build src/** -f html -o docs",
    "build:js": "cross-env NODE_ENV=production babel src -d lib",
    "clean": "rimraf docs lib",
    "lint": "npm-run-all -p lint:*",
    "lint:documentation": "documentation lint src",
    "lint:es": "eslint . --cache",
    "lint:flow": "flow",
    "postpublish": "ghpages --path docs",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "start": "node lib/index.js",
    "test": "npm-run-all -p test:*",
    "test:lint": "npm run lint"
  }
}
