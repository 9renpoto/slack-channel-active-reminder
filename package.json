{
  "name": "slack-channel-active-reminder",
  "description": "remind non acctive channel at slack",
  "version": "0.3.0",
  "author": "github.com/9renpoto",
  "bugs": {
    "url": "https://github.com/9renpoto/slack-channel-active-reminder/issues"
  },
  "dependencies": {
    "@slack/client": "4.8.0",
    "moment": "2.22.2"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.7.0",
    "@9renpoto/tslint-config": "4.7.0",
    "cross-env": "5.2.0",
    "eslint": "5.9.0",
    "ghpages": "0.0.10",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2",
    "tslint": "5.11.0",
    "typedoc": "0.13.0",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "21.0.2"
  },
  "engines": {
    "node": ">=7.10"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/9renpoto/slack-channel-active-reminder#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "slack"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "prettier-eslint --write",
      "eslint --ext=ts",
      "git add"
    ],
    "*.{md,json,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/slack-channel-active-reminder.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:docs": "typedoc --out docs",
    "build:ts": "tsc",
    "clean": "rimraf lib",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --cache --ext=ts",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "start": "node lib/index.js",
    "test": "run-p test:*",
    "test:lint": "npm run lint"
  }
}
