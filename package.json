{
  "name": "slack-channel-active-reminder",
  "description": "remind non acctive channel at slack",
  "version": "0.3.0",
  "author": "github.com/9renpoto",
  "bugs": {
    "url": "https://github.com/9renpoto/slack-channel-active-reminder/issues"
  },
  "dependencies": {
    "@slack/client": "4.0.0",
    "config": "2.0.0",
    "moment": "2.20.1"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "4.7.0",
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-flow": "7.0.0",
    "@babel/register": "7.0.0",
    "cross-env": "5.2.0",
    "documentation": "8.1.2",
    "eslint": "5.9.0",
    "flow-bin": "0.86.0",
    "ghpages": "0.0.10",
    "husky": "1.1.4",
    "lint-staged": "8.0.5",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2"
  },
  "engines": {
    "node": ">=7.10"
  },
  "files": ["lib"],
  "homepage":
    "https://github.com/9renpoto/slack-channel-active-reminder#readme",
  "keywords": ["slack"],
  "license": "MIT",
  "lint-staged": {
    "*.js": ["prettier-eslint --write", "eslint --cache", "git add"],
    "*.{md,json,yml}": ["prettier --write", "git add"]
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/slack-channel-active-reminder.git"
  },
  "scripts": {
    "build": "mkdir -p docs lib && run-p build:*",
    "build:doc": "documentation build src/** -f html -o docs",
    "build:js": "cross-env NODE_ENV=production babel src -d lib",
    "clean": "rimraf docs lib",
    "lint": "run-p lint:*",
    "lint:documentation": "documentation lint src",
    "lint:es": "eslint . --cache",
    "lint:flow": "flow",
    "postpublish": "ghpages --path docs",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "start": "node lib/index.js",
    "test": "run-p test:*",
    "test:lint": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
